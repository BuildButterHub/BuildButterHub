--Build Butter Hub

--Version 1.0.3



--Teleport To Team
function TeleportTeamFunction(team)
if team == "White Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-53.5656891, 20.9999924, -345.506866, 1, 0, 0, 0, 1, 0, 0, 0, 1)
elseif team == "Black Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-328.965668, 20.9999924, -72.1068497, 0, 0, 1, 0, 1, -0, -1, 0, 0)
elseif team == "Red Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(221.83432, 20.9999924, -68.7067871, 0, 0, -1, 0, 1, 0, 1, 0, 0) 
elseif team == "Green Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-328.965668, 20.9999924, 285.893158, 0, 0, 1, 0, 1, -0, -1, 0, 0)
elseif team == "Blue Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(221.83432, 20.9999924, 289.493195, 0, 0, -1, 0, 1, 0, 1, 0, 0)
elseif team == "Yellow Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-328.965668, 20.9999924, 643.893188, 0, 0, 1, 0, 1, -0, -1, 0, 0)
elseif team == "Mangenta Team" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(221.83432, 20.9999924, 647.693176, 0, 0, -1, 0, 1, 0, 1, 0, 0)
end
end
--end





--AutoBuy

function AutoBuyFunction(Item,Amount)
if Item == "WoodBlock" then local viScript = {[1] = "WoodBlock",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript)) 
    elseif Item == "WoodRod" then local viScript = {[1] = "WoodRod",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "SmoothWoodBlock" then local viScript = {[1] = "SmoothWoodBlock",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Wedge" then local viScript = {[1] = "Wedge", [2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "CornerWedge" then local viScript = {[ 1] = "CornerWedge",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Seat" then local viScript = {[1] = "Seat",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Step" then local viScript = {[1] = "Step",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Truss" then local viScript = {[1] = "Truss",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Mast" then local viScript = {[1] = "Mast",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "StreeringWheel" then local viScript = {[1] = "StreeringWheel",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Window" then local viScript = {[1] = "Window",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Door" then local viScript = {[1] = "Door",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Hatch" then local viScript = {[1] = "Hatch",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "SpikeTrap" then local viScript = {[1] = "SpikeTrap",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "PineTree" then local viScript = {[1] = "PineTree",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "GlassBlock" then local viScript = {[1] = "GlassBlock",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "StoneBlock" then local viScript = {[1] = "StoneBlock",[2] = Amount} 
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "StoneRod" then local viScript = {[1] = "StoneRod",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Torch" then local viScript = {[1] = "Torch",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Lamp" then local viScript = {[1] = "Lamp",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "Candle" then local viScript = {[1] = "Candle",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    elseif Item == "RustedBlock" then local viScript = {[1] = "RustedBlock",[2] = Amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(viScript))
    end
    notifications:message{Title = "Build Butter Hub",Description = "Done",Icon = 6023426926}
end


function ChestFunction(chest,amount)
if chest == "Common Chest" then local VinChestBuy = {[1] = "Common Chest",[2] = amount} 
    workspace.ItemBoughtFromShop:InvokeServer(unpack(VinChestBuy))
  elseif chest == "Uncommon Chest" then local VinChestBuy = {[1] = "Uncommon Chest", [2] = amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(VinChestBuy))
  elseif chest == "Rare Chest" then local VinChestBuy = {[1] = "Rare Chest", [2] = amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(VinChestBuy))
  elseif chest == "Epic Chest" then local VinChestBuy = {[1] = "Epic Chest", [2] = amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(VinChestBuy))
  elseif chest == "Legendary Chest" then local VinChestBuy = {[1] = "Legendary Chest", [2] = amount}
    workspace.ItemBoughtFromShop:InvokeServer(unpack(VinChestBuy))
  end
end
--end

--Library
local BBH = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))() 
local notifications = loadstring(game:HttpGet(("https://raw.githubusercontent.com/AbstractPoo/Main/main/Notifications.lua"),true))()

local window = BBH.CreateLib("Build Butter Hub","Midnight")

--end

--Tab local
local Tab1 = window:NewTab("Home")
local Section1 = Tab1:NewSection("Information")
local Tab2 = window:NewTab("Player")
local Section2 = Tab2:NewSection("Basic")
local Tab8 = window:NewTab("World")
local Section8 = Tab8:NewSection("Teleport")
local Tab4 = window:NewTab("Quests")
local Section4 = Tab4:NewSection("Current")
local Tab3 = window:NewTab("AutoFarm")
local Section3 = Tab3:NewSection("AutoFarm")
local Tab6 = window:NewTab("AutoBuy")
local Section6 = Tab6:NewSection("Item")
--end

--Locals

local ItemToBuy local ValueDrop = 1 local ChestBuy local ValueChest = 1 local statuse = true local status = false
local jumpPowers = 50 local speedwalk = 16


--Home

Section1:NewLabel("Version 1.0.3")
Section1:NewLabel("Discrord https://discord.gg/KbKQJVpfSn")

--end

--Player

Section2:NewSlider("Speed", "", 250, 16, function(SpeedPlayerStat)
    speedwalk = SpeedPlayerStat
        
    end)
    
    Section2:NewToggle("Speed On/Off", "", function(state)
        if state then
          game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speedwalk
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end)


    Section2:NewSlider("Jump", "", 250, 50, function(JumpPlayerStat)
    jumpPowers = JumpPlayerStat
        end)

        Section2:NewToggle("Jump On/Off", "", function(state)
            if state then
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = jumpPower
            else
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
            end
        end)


Section2:NewLabel("Other")

local TeleportTeam


--World

Section8:NewDropdown("Telepot To team", "", {"White Team", "Black Team", "Red Team","Green Team", "Blue Team", "Yellow Team", "Mangenta Team"}, function(Value)
    TeleportTeamFunction(Value)
    notifications:message{Title = "Build Butter Hub",Description = "Teleported",Icon = 6023426926}
end)
Section8:NewLabel("World")

--end



--Quests
local QuestsLocal local Quests
Section4:NewDropdown("Select. . .", "", {"Cloud", "Target", "Soccer", "Ramp", "Find Me", "Dragon", "The Box", "Thin Ice",}, function(Value)
    Quests = Value
    if Quests == "Cloud" then
        QuestsLocal = "1"
    elseif Quests == "Target" then
        QuestsLocal = "2"
    elseif Quests == "Soccer" then
        QuestsLocal = "8"
    elseif Quests == "Ramp" then
        QuestsLocal = "3"
    elseif Quests == "Find Me" then
        QuestsLocal = "4"
    elseif Quests == "Dragon" then
        QuestsLocal = "5"
    elseif Quests == "The Box" then
        QuestsLocal = "6"
    elseif Quests == "Thin Ice" then
        QuestsLocal = "9"
    end
end)

Section4:NewButton("Start", "", function()
    notifications:message{Title = "Build Butter Hub",Description = "Quest Started",Icon = 6023426926}
    if QuestsLocal == "1" then
        workspace.QuestMakerEvent:FireServer(1)
    elseif QuestsLocal == "2" then
        workspace.QuestMakerEvent:FireServer(2)
    elseif QuestsLocal == "8" then
        workspace.QuestMakerEvent:FireServer(8)
    elseif QuestsLocal == "3" then
        workspace.QuestMakerEvent:FireServer(3)
    elseif QuestsLocal == "4" then
        workspace.QuestMakerEvent:FireServer(4)
    elseif QuestsLocal == "5" then
        workspace.QuestMakerEvent:FireServer(5)
    elseif QuestsLocal == "6" then
        workspace.QuestMakerEvent:FireServer(6)
    elseif QuestsLocal == "9" then
        workspace.QuestMakerEvent:FireServer(9)
    end
end)

Section4:NewButton("Remove Event", "", function()
    notifications:message{Title = "Build Butter Hub",Description = "Quest Stoped",Icon = 6023426926}
    workspace.QuestMakerEvent:FireServer(0)
end)
Section4:NewLabel("Lost")

Section4:NewButton("Fabbi Boss", "", function()
notifications:message{Title = "Build Butter Hub",Description = "Quest Started",Icon = 6023426926}
    workspace.QuestMakerEvent:FireServer(100)
end)

Section4:NewButton("Crismas", "", function()
notifications:message{Title = "Build Butter Hub",Description = "Quest Started",Icon = 6023426926}
    workspace.QuestMakerEvent:FireServer(99)
end)

--end

-- AutoFarm

Section3:NewButton("Start Farm", "", function()
    notifications:message{Title = "Build Butter Hub",Description = "Farm Started",Icon = 6023426926}
status = true

    while status do
        print("Gravitation off")
        game.workspace.Gravity = 2
print("1 LVL")


game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 1369.09009, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(4)
game.workspace.Gravity = 162
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-55.7065125, -357.739624, 9492.35645, 0, 0, -1, 0, 1, 0, 1, 0, 0)
wait(0.7)
game.workspace.Gravity = 2
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 2139.09009, 1, 0, 0, 0, 1, 0, 0, 0, 1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 2909.09009, 1, 0, 0, 0, 1, 0, 0, 0, 1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 3679.09009, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 4449.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 5219.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 6759.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 5989.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(0.5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 7529.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(0.7)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-51.5656433, 65.0000458, 8299.08984, 1, 0, 0, 0, 1, 0, 0, 0, 1)
wait(0.5)
print("Teleport Suspen")
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(469.263947, -10.100008, 616.786743, 1, 0, 0, 0, 1, 0, 0, 0, 1)

workspace.ClaimRiverResultsGold:FireServer()
             print("on")
             wait(7)
             end
    end)

Section3:NewButton("Stop Farm", "", function()
status = false
        print("off")
        FarmStat = false
        game.workspace.Gravity = 162
        workspace.ClaimRiverResultsGold:FireServer()
notifications:message{Title = "Build Butter Hub",Description = "Farm Stoped",Icon = 6023426926}
    end)





--end

--AutoBuy
Section6:NewDropdown("Select. . .", "", {"WoodBlock", "WoodRod", "SmoothWoodBlock", "Wedge", "CornerWedge", "Seat", "Step", "Truss", "StreeringWheel", "Window", "Door", "Hatch", "SpikeTrap", "PineTree", "GlassBlock", "StoneBlock", "StoneRod", "Lamp", "Candle", "RustedBlock"}, function(Value)
    ItemToBuy = Value
    print(ItemToBuy)
    end)
    
    Section6:NewSlider("Amount", "", 100, 1, function(Values)
        ValueDrop = Values
    end)


Section6:NewButton("Phurchase Selected Item(s)", "", function()
    print(ItemToBuy , ValueDrop)
AutoBuyFunction(ItemToBuy,ValueDrop)
    end)
    Section6:NewLabel("Chest")

    Section6:NewDropdown("Select. . .", "", {"Common Chest", "Uncommon Chest", "Rare Chest", "Epic Chest", "Legendary Chest"}, function(Value)
        ChestBuy = Value
        end)

        Section6:NewSlider("Amount", "", 100, 1, function(Values)
            ValueChest = Values
        end)

Section6:NewButton("Phurchase Selected Item(s)", "", function()
ChestFunction(ChestBuy,ValueChest)
  notifications:message{Title = "Build Butter Hub",Description = "Done",Icon = 6023426926}
end)

